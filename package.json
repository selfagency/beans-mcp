{
  "name": "beans-mcp-server",
  "version": "0.1.0",
  "description": "MCP (Model Context Protocol) server for Beans issue tracker",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "beans-mcp-server": "./dist/beans-mcp-server.cjs"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "beans",
    "mcp",
    "model-context-protocol",
    "issue-tracker",
    "ai"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.27.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "tsup": "8.5.1",
    "@types/node": "^20.19.0",
    "@vitest/ui": "4.0.18",
    "typescript": "^5.9.3",
    "vitest": "4.0.18"
  }
}